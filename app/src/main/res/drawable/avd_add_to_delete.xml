<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:aapt="http://schemas.android.com/aapt">
  <aapt:attr name="android:drawable">
    <vector
      android:name="add_to_delete"
      android:width="24dp"
      android:height="24dp"
      android:viewportWidth="24"
      android:viewportHeight="24">
      <path
        android:name="trash_lid_handle"
        android:pathData="M 7 12 L 10 12 L 14 12 L 17 12"
        android:strokeWidth="2"
        android:strokeColor="#000000" />
      <path
        android:name="trash_lid"
        android:fillColor="#000"
        android:pathData="M 4 12 L 20 12"
        android:strokeWidth="2"
        android:strokeColor="#000000" />
      <path
        android:name="vertical_02"
        android:pathData="M 12 4 L 12 20"
        android:strokeWidth="2"
        android:strokeColor="#000000" />
      <path
        android:name="vertical_01"
        android:pathData="M 12 4 L 12 20"
        android:strokeWidth="2"
        android:strokeColor="#000000" />
      <group android:name="trash_can_translate">
        <path
          android:name="trash_can"
          android:pathData="M 6 6 L 6 20 L 18 20 L 18 6"
          android:strokeWidth="2"
          android:strokeColor="#000000"
          android:strokeLineJoin="round"
          android:trimPathEnd="0" />
      </group>
    </vector>
  </aapt:attr>
  <target android:name="vertical_01">
    <aapt:attr name="android:animation">
      <set>
        <objectAnimator
          android:duration="100"
          android:interpolator="@android:interpolator/fast_out_linear_in"
          android:propertyName="pathData"
          android:valueFrom="M 12 4 L 12 20"
          android:valueTo="M 12 9 L 12 19.5"
          android:valueType="pathType" />
        <objectAnimator
          android:duration="150"
          android:interpolator="@android:interpolator/linear_out_slow_in"
          android:propertyName="pathData"
          android:startOffset="100"
          android:valueFrom="M 12 9 L 12 19.5"
          android:valueTo="M 10 9 L 10 17"
          android:valueType="pathType" />
      </set>
    </aapt:attr>
  </target>
  <target android:name="trash_lid">
    <aapt:attr name="android:animation">
      <objectAnimator
        android:duration="300"
        android:interpolator="@android:interpolator/fast_out_slow_in"
        android:propertyName="pathData"
        android:valueFrom="M 4 12 L 20 12"
        android:valueTo="M 4 6 L 20 6"
        android:valueType="pathType" />
    </aapt:attr>
  </target>
  <target android:name="vertical_02">
    <aapt:attr name="android:animation">
      <set>
        <objectAnimator
          android:duration="100"
          android:interpolator="@android:interpolator/fast_out_linear_in"
          android:propertyName="pathData"
          android:valueFrom="M 12 4 L 12 20"
          android:valueTo="M 12 9 L 12 19.5"
          android:valueType="pathType" />
        <objectAnimator
          android:duration="150"
          android:interpolator="@android:interpolator/linear_out_slow_in"
          android:propertyName="pathData"
          android:startOffset="100"
          android:valueFrom="M 12 9 L 12 19.5"
          android:valueTo="M 14 9 L 14 17"
          android:valueType="pathType" />
      </set>
    </aapt:attr>
  </target>
  <target android:name="trash_can">
    <aapt:attr name="android:animation">
      <objectAnimator
        android:duration="300"
        android:interpolator="@android:interpolator/fast_out_slow_in"
        android:propertyName="trimPathEnd"
        android:valueFrom="0"
        android:valueTo="1"
        android:valueType="floatType" />
    </aapt:attr>
  </target>
  <target android:name="trash_can_translate">
    <aapt:attr name="android:animation">
      <objectAnimator
        android:duration="250"
        android:interpolator="@android:interpolator/fast_out_slow_in"
        android:propertyName="translateY"
        android:valueFrom="6"
        android:valueTo="0"
        android:valueType="floatType" />
    </aapt:attr>
  </target>
  <target android:name="trash_lid_handle">
    <aapt:attr name="android:animation">
      <objectAnimator
        android:duration="300"
        android:interpolator="@android:interpolator/fast_out_slow_in"
        android:propertyName="pathData"
        android:valueFrom="M 7 12 L 10 12 L 14 12 L 17 12"
        android:valueTo="M 9 6 L 10 3 L 14 3 L 15 6"
        android:valueType="pathType" />
    </aapt:attr>
  </target>
</animated-vector>
